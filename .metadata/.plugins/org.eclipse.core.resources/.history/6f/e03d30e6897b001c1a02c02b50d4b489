public class hello
{
	public static void main(String[] args)
	{
		
		/*想做出一個可以紀錄骰子點數的東西*/
		int times   = 100;
		int[]record = new int[6];
		
		for(int i=0;i<times;i++) {
			int dice = (int) (Math.random()*5.99999);
			record[dice]++;
			
		}
		
		System.out.println("one :"+record[0]+"=");
		
		
		
	}
}