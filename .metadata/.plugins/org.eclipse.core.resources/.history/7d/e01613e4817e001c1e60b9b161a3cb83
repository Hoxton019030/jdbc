package cycu;

public class sodadasd {
	public static void main(String[] args) {

		int[] list = { 5, 3, 2, 1, 4 };
		int list_length = list.length;
		
		for(int time=0; time==(1+list_length)/2; time++ )
		{
			for(int i=0,y=5;i<y;y--)
			{
				if(list[i]>list[i+1])
				{
					int tem = list[i];
					list[i]=list[i+1];
					list[i+1] = tem;
				}
				
				
			}
		}
		for(int i=0;i<list_length;i++)
		System.out.println(list[i]);
	}

}
