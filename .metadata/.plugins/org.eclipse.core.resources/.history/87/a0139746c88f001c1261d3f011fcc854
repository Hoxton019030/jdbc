package 進階;

public class martrixver_2 {
	public static void main(String[] args) {
		
	int[][] array = {{1,2},{3,4},{5,6},{7,8}}; //4x2//
	
	int[][] new_array = new int[2][4];
	
	for (int i=0;i<2;i++) {
		for(int j = 0 ; j <4 ; j++) {
			new_array[j][i]= array[i][j];
		}
	}
	for (int k =0;k<4;k++) {
		for(int p = 0 ; p <2 ; p++) {
			System.out.println(new_array[k][p]);
		}
	}
	
		
	}
	

}
