
public class classSolution {
	public static void main(String[] args) {
		int[] nums = {1,2,1,3,4,5,6,7};
		int[] ans = new int[nums.length*2];
		for(int i=0;i<nums.length;i++) {
			ans[i]=nums[i];
		}
		for(int i=8;i<ans.length;i++) {
			ans[i]=nums[i-8];
		}
		
		for(int i : ans) {
			System.out.println(i);
		}
		
		
		
	}

}
