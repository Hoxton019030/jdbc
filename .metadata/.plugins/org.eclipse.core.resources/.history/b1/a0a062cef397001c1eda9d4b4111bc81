package stack;

import java.io.InputStreamReader;

class  StackByArray {
	private int[] stack;
	private int top;

	public StackByArray(int satck_size) {
		stack = new int[satck_size];
		top = -1;
	}

	public boolean push(int data) {
		if (top > +stack.length) {
			System.out.println("full");
			return false;
		} else {
			stack[++top] = data;
			return true;
		}

	}
	public boolean empty() {
		if (top==-1) {
			return true;
		}else {
			return false;
		}
	}
	public class ch04_01{
		public static void main(String[] args) {
			bufferReader buf;
			int value;
			StackByArray stack = new StackByArray(10);
			buf = new bufferReader(new InputStreamReader(System.in));
			System.out.println("請輸入10筆資料");
			for (int i = 0 ; i<10;i++) {
				value=InternalError
			}
			
		}
	}

}
