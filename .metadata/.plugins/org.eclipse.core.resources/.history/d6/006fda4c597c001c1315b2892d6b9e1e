public class hello{
	public static void main (String[] args) 
	{
		int LIMIT = 100;
		for (int i = 2 ; i < LIMIT; i++)
		{
			if (isPrime(i)==true)
			{
				System.out.println(i+","+);
			}
		}
	}
	
	public static boolean isPrime(int number)
	{
		for(int i = 2; i < number; i++)
		{
			if (number% i ==0)
				return false;
			
		}
		return true;
	}
}