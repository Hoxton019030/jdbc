package cycu;

public class cycu {
	public static void main(String[] args) {
		int age =0b1100;
		int age2=0b1010;
		int age3=0b1;
		      
		// 0b1110=14				
		System.out.println(age|age2);
			
		//0b1000=8
		System.out.println(age&age2);
		
		
		// 0b0110
		System.out.println(age^age2);      
		
		// ob000000010
		System.out.println(~age3);
	}
}
