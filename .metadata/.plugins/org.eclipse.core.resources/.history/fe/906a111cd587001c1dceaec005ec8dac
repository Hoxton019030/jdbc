package extendDemo;

class CScore {
	private int chia;
	private int math;

	CScore() {
		chia = 0;
		math = 0;
	}

	CScore(int chia, int math) {
		this.chia = chia;
		this.math = math;
	}

	public void showScore() {
		System.out.println("國文:" + this.chia + "\t 數學:" + this.math);
	}
}

class SonCScore extends CScore {
	private int eng;

	SonCScore(){
		super();
		eng = 0;
	}
	SonCScore(int chia,int math, int eng){
		super(chia,math);
		this.eng = eng;
	}
}

public class SuperDemo {

}
